{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "react",
  "functions": {
    "backend/src/index.ts": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
